<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>

<fx:root prefWidth="600.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="name" prefWidth="100.0" style="-fx-padding: 5px;" text="Название" />
      <Separator orientation="VERTICAL" />
      <Label fx:id="date" prefWidth="120.0" style="-fx-padding: 5px;" text="Дата" />
      <Separator orientation="VERTICAL" />
      <Label fx:id="stationCount" prefWidth="110.0" style="-fx-padding: 5px;" text="Кол станций" />
      <Separator orientation="VERTICAL" />
      <Label fx:id="factoryCount" prefWidth="130.0" style="-fx-padding: 5px;" text="Кол. производств" />
      <Separator orientation="VERTICAL" />
      <Label fx:id="author" prefWidth="120.0" style="-fx-padding: 5px;" text="Создатель" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <styleClass>
      <String fx:value="list-string" />
      <String fx:value="list-string-project" />
   </styleClass>
</fx:root>
